{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"module": "CommonJS",
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"target": "esnext",
		"lib": ["ESNext", "ESNext.Disposable", "DOM"],
		"sourceMap": true,
		"strict": true,
		"skipLibCheck": true,
		"useUnknownInCatchVariables": false
	}
}
