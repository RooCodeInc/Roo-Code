+			"cache": false,
+			"dependsOn": [
+				"clean:resource-host",
+				"@kilo-code/jetbrains-host#bundle",
+				"@kilo-code/jetbrains-host#prop:deps"
+			]
+		},
+		"copy:resource-logs": {
+			"cache": false,
+			"dependsOn": ["clean:resource-logs"]
+		},
+		"copy:resource-nodemodules": {
+			"cache": false,
+			"inputs": ["../host/package.json", "../host/package-lock.json"],
