+-async function mkdirpIgnoreError(dir: string | undefined): Promise<string | undefined> {
+-	if (typeof dir === 'string') {
+-		try {
+-			await fs.promises.mkdir(dir, { recursive: true });
+-
+-			return dir;
+-		} catch (error) {
+-			// ignore
+-		}
+-	}
+-
+-	return undefined;
+-}
