+    val shellPath: String? = null,
+    val shellArgs: List<String>? = null,
+    val cwd: String? = null,
+    val env: Map<String, String>? = null,
+    val useShellEnvironment: Boolean? = null,
+    val hideFromUser: Boolean? = null,
+    val isFeatureTerminal: Boolean? = null,
