+
+data class FileEdit(
+    val oldResource: URI?,
+    val newResource: URI?,
+    val options: Options?,
+    val metadata: Metadata?
+) {
+    data class Options(
+        val overwrite: Boolean?,
+        val ignoreIfExists: Boolean?,
+        val copy: Boolean?,
+        val contents: String?
+    )
+}
+
+data class Metadata(
+    val isRefactoring: Boolean?
+)
