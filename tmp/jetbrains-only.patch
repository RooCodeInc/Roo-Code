+data class DocumentsAndEditorsState (
+    var editors: MutableMap<String , TextEditorAddData> =  ConcurrentHashMap(),
+    var documents: MutableMap<URI, ModelAddedData> =  ConcurrentHashMap(),
+    var activeEditorId: String? = null
